{
   "name": "PasswordHighlighter",
   "version": "1.0",
   "manifest_version": 2,
   "page_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   },
   "content_scripts": [
      {
         "matches":["http://*/*", "https://*/*"],
         "js": ["content.js"]
      }
   ],
   "background": {
      "scripts": ["background.js"],
      "persistent": false
   }
   
}
